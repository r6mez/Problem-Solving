        "scope": "cpp",
        "prefix": "nCr",
        "body": [
            "int nCr(int n, int r) {",
            "    int ans = 1;",
            "    int div = 1; // r!",
            "    for (int i = r + 1; i <= n; i++) {",
            "        ans = ans * i;",
            "        ans /= div;",
            "        div++;",
            "    }",
            "    return ans;",
            "}"
        ],
        "description": "Returns the number of combinations nCr"
