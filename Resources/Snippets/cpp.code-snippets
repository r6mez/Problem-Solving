            "int pwmod(int a, int b, int m) {",
            "    a %= m;",
            "    int result = 1;",
            "    while (b > 0) {",
            "        if (b & 1) result = (result * a) % m;",
            "        a = (a * a) % m;",
            "        b /= 2;",
            "    }",
            "    return result;",
            "}",
