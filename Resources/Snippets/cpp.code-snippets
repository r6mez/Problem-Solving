            "    vector<vi> b(a.size(), vi(a[0].size()));",
            "    for (int i = 0; i < a.size(); i++) {",
            "        partial_sum(all(a[i]), b[i].begin());",
            "    }",
            "    for (int i = 0; i < a[0].size(); i++) {",
            "        for (int j = 1; j < a.size(); j++) {",
